{
  "name": "@rarimo/passport-voting-contracts",
  "version": "1.1.0",
  "license": "MIT",
  "author": "Zero Block Global Foundation",
  "description": "Decentralized Identity Voting",
  "keywords": [
    "solidity",
    "ethereum",
    "smart-contracts",
    "zero-knowledge",
    "passports"
  ],
  "files": [
    "**/*.sol",
    "!mock/**/*"
  ],
  "scripts": {
    "prepare": "husky",
    "compile": "npx hardhat compile",
    "coverage": "npx hardhat coverage --solcoverjs ./.solcover.ts",
    "clean": "npx hardhat clean",
    "test": "npx hardhat test",
    "test:realproof": "npx hardhat test test/voting/BioPassportVoting.realproof.test.ts",
    "test:integration": "npx hardhat test test/voting/PassportIntegration.test.ts",
    "test-all": "npx hardhat migrate && npm run test",
    "private-network": "npx hardhat node",
    "private-network-fork": "npx hardhat node --fork https://mainnet.infura.io/v3/$(grep INFURA_KEY .env | cut -d '\"' -f2)",
    "deploy-localhost": "npx hardhat migrate --network localhost",
    "seed:local": "npx hardhat run scripts/seed-local.ts --network localhost",
    "deploy-sepolia": "npx hardhat migrate --network sepolia --verify",
    "deploy-rarimo-l2": "npx hardhat migrate --network rarimo-l2 --verify",
    "deploy-rarimo-l2-testnet": "npx hardhat migrate --network rarimo-l2-testnet --verify",
    "deploy-rarimo-beta": "npx hardhat migrate --network rarimo-beta --verify",
    "deploy-q-testnet": "npx hardhat migrate --network q-testnet --verify",
    "generate-types": "TYPECHAIN_FORCE=true npx hardhat typechain && npx hardhat gobind",
    "generate-docs": "npx hardhat markup",
    "lint-fix": "npm run lint-sol-fix && npm run lint-ts-fix && npm run lint-json-fix",
    "lint-json-fix": "prettier --write \"./**/*.json\"",
    "lint-ts-fix": "prettier --write \"./**/*.ts\"",
    "lint-sol-fix": "prettier --write \"contracts/**/*.sol\"",
    "publish-to-npm": "npm run lint-fix && bash ./scripts/publish.sh --public"
  },
  "dependencies": {
    "@openzeppelin/contracts": "4.9.6",
    "@openzeppelin/contracts-upgradeable": "4.9.6",
    "@rarimo/passport-contracts": "0.4.6",
    "@solarity/solidity-lib": "2.7.10",
    "poseidon-solidity": "0.0.5"
  },
  "devDependencies": {
    "@iden3/js-crypto": "^1.0.3",
    "@metamask/eth-sig-util": "^7.0.1",
    "@noir-lang/backend_barretenberg": "^0.30.0",
    "@noir-lang/noir_js": "1.0.0-beta.1",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.6",
    "@nomicfoundation/hardhat-ethers": "^3.0.5",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-verify": "^2.1.0",
    "@peculiar/asn1-cms": "^2.6.1",
    "@peculiar/asn1-rsa": "^2.6.0",
    "@peculiar/asn1-schema": "^2.6.0",
    "@peculiar/asn1-x509": "^2.6.0",
    "@solarity/hardhat-gobind": "^1.2.2",
    "@solarity/hardhat-markup": "^1.0.7",
    "@solarity/hardhat-migrate": "^3.0.2",
    "@trezor/connect": "9.5.0",
    "@typechain/ethers-v6": "^0.5.1",
    "@typechain/hardhat": "^9.1.0",
    "@types/chai": "^4.3.14",
    "@types/mocha": "^10.0.6",
    "@types/node": "^18.16.0",
    "chai": "^4.4.1",
    "circomlibjs": "^0.1.7",
    "dotenv": "^16.4.5",
    "ethers": "^6.11.1",
    "hardhat": "^2.20.1",
    "hardhat-contract-sizer": "^2.10.0",
    "hardhat-gas-reporter": "^2.0.2",
    "husky": "^9.0.11",
    "merkletreejs": "^0.3.11",
    "mocha": "^10.3.0",
    "prettier": "^3.2.5",
    "prettier-plugin-solidity": "^1.3.1",
    "snarkjs": "^0.7.6",
    "solhint": "^4.1.1",
    "solhint-plugin-prettier": "^0.1.0",
    "solidity-coverage": "^0.8.11",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typechain": "^8.3.2",
    "typescript": "^5.4.3"
  }
}
